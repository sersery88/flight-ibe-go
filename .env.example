# Amadeus API Credentials
AMADEUS_CLIENT_ID=your_client_id_here
AMADEUS_CLIENT_SECRET=your_client_secret_here
AMADEUS_ENV=test

# Server Configuration
PORT=8080
ENVIRONMENT=development

# Cache Configuration
CACHE_SIZE=1000
CACHE_TTL=15m

# Rate Limiting
RATE_LIMIT_RPS=10
RATE_LIMIT_BURST=20

# Logging
LOG_LEVEL=debug

# Observability
ENABLE_TRACING=false
ENABLE_METRICS=true
OTLP_ENDPOINT=
